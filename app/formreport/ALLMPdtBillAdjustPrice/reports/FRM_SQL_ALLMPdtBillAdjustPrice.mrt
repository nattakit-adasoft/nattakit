        "SqlCommand": "declare @ptDocNo varchar(20) = '{SP_tDocNo}' \r\n declare @pnLanguage int = '{nLanguage}' \r\n SELECT MstAdjP.FTBchCode, MstAdjP.FTXphDocNo, MstAdjP.FNXpdSeq,\r\n\tMstAdjP.FTPdtCode, OldAdjP.FCXpdPriceRet AS FCXpdPriceRetOld,\r\n\tOldAdjP.FCXpdPriceWhs AS FCXpdPriceWhsOld, OldAdjP.FCXpdPriceNet AS\r\n\tFCXpdPriceNetOld, MstAdjP.FCXpdPriceRet, MstAdjP.FCXpdPriceWhs,\r\n\tMstAdjP.FCXpdPriceNet, MstAdjP.FTXpdShpTo, MstAdjP.FTXpdBchTo,\r\n\tMstAdjP.FDLastUpdOn, MstAdjP.FTLastUpdBy, MstAdjP.FTPdtName,\r\n\tMstAdjP.FTPunCode, MstAdjP.FTPunName\r\n\tFROM (SELECT TCNTPdtAdjPriDT.FTBchCode, TCNTPdtAdjPriDT.FTXphDocNo,\r\n\tTCNTPdtAdjPriDT.FNXpdSeq, TCNTPdtAdjPriDT.FTPdtCode, TCNTPdtAdjPriDT.FCXpdPriceRet,\r\n\tTCNTPdtAdjPriDT.FCXpdPriceWhs, TCNTPdtAdjPriDT.FCXpdPriceNet,\r\n\tTCNTPdtAdjPriDT.FTXpdShpTo, TCNTPdtAdjPriDT.FTXpdBchTo,\r\n\tTCNTPdtAdjPriDT.FDLastUpdOn, TCNTPdtAdjPriDT.FTLastUpdBy, Pdt.FTPdtName,\r\n\tTCNTPdtAdjPriDT.FTPunCode, Pun.FTPunName \r\n\tFROM TCNTPdtAdjPriDT WITH (NOLOCK) INNER JOIN\r\n      TCNMPdt_L Pdt WITH (NOLOCK) ON TCNTPdtAdjPriDT.FTPdtCode = Pdt.FTPdtCode LEFT OUTER JOIN\r\n      TCNMPdtUnit_L Pun WITH (NOLOCK) ON TCNTPdtAdjPriDT.FTPunCode = Pun.FTPunCode AND\r\n        Pun.FNLngID = @pnLanguage\r\n    WHERE TCNTPdtAdjPriDT.FTXphDocNo = @ptDocNo) MstAdjP LEFT JOIN\r\n(SELECT AdjP.FTBchCode, AdjP.FDXphDocDate, AdjP.FTPdtCode, AdjP.FTPunCode,\r\n\tAdjP.FCXpdPriceRet, AdjP.FCXpdPriceWhs, AdjP.FCXpdPriceNet\r\n\tFROM (SELECT DT.FTBchCode, DT.FTXphDocNo, HD.FDXphDocDate, DT.FNXpdSeq,\r\n\tDT.FTPdtCode, DT.FTPunCode, DT.FCXpdPriceRet, DT.FCXpdPriceWhs,\r\n\tDT.FCXpdPriceNet\r\n\tFROM TCNTPdtAdjPriDT DT WITH (NOLOCK) \r\n\t\t  INNER JOIN TCNTPdtAdjPriHD HD WITH (NOLOCK) ON DT.FTBchCode = HD.FTBchCode AND DT.FTXphDocNo = HD.FTXphDocNo \r\n\t\t  LEFT OUTER JOIN TCNMPdtUnit_L Pun WITH (NOLOCK) ON DT.FTPunCode = Pun.FTPunCode AND Pun.FNLngID = @pnLanguage\r\n        WHERE DT.FTXphDocNo <> @ptDocNo AND CONVERT(VARCHAR,HD.FDXphDStop +\r\n          ' ' + HD.FTXphTStop,120) < CONVERT(VARCHAR,getdate(),120) AND\r\n          ISNULL(HD.FTXphUsrApv, '') <> '') AdjP INNER JOIN\r\n(SELECT Max(HD.FDXphDocDate) AS FDXphDocDate, DT.FTPdtCode, DT.FTPunCode\r\n\tFROM TCNTPdtAdjPriDT DT WITH (NOLOCK)\r\n\t\t\tINNER JOIN   TCNTPdtAdjPriHD HD WITH (NOLOCK) ON DT.FTBchCode = HD.FTBchCode AND DT.FTXphDocNo = HD.FTXphDocNo\r\n        WHERE CONVERT(VARCHAR,HD.FDXphDStop + ' ' + HD.FTXphTStop,120) <\r\n          CONVERT(VARCHAR,getdate(),120) AND ISNULL(HD.FTXphUsrApv, '') <> ''\r\n          AND DT.FTXphDocNo <> @ptDocNo\r\n        GROUP BY DT.FTPdtCode, DT.FTPunCode) DateMax ON AdjP.FDXphDocDate =\r\n          DateMax.FDXphDocDate AND AdjP.FTPdtCode = DateMax.FTPdtCode AND\r\n          AdjP.FTPunCode = DateMax.FTPunCode\r\nWHERE AdjP.FDXphDocDate <= (SELECT TCNTPdtAdjPriHD.FDXphDocDate\r\n\tFROM TCNTPdtAdjPriHD WITH (NOLOCK)\r\n        WHERE TCNTPdtAdjPriHD.FTXphDocNo = @ptDocNo)) OldAdjP ON MstAdjP.FTPdtCode\r\n      = OldAdjP.FTPdtCode AND MstAdjP.FTPunCode = OldAdjP.FTPunCode",
        "Parameters": {
          "0": {
            "Name": "ptDocNo",
            "Expression": "SP_tDocNo",
            "Type": 22,
            "Size": 20
          },
          "1": {
